{
  "name": "damianulan/laraverse",
  "type": "library",
  "description": "Laraverse Laravel skeleton application package",
  "keywords": [
    "laravel",
    "documentation",
    "markdown",
    "module"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Damian UÅ‚an",
      "email": "damian.ulan@protonmail.com",
      "homepage": "https://damianulan.me",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.3",
    "ext-json": "*",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
    "mews/purifier": "^3.4",
    "spatie/laravel-markdown": "*",
    "spatie/shiki-php": "^2.3"
  },
  "require-dev": {
    "mockery/mockery": "^1.5",
    "phpunit/phpunit": "^11.0|^12.0"
  },
  "suggest": {
    "laravel/framework": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "Laraverse\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Laraverse\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Laraverse\\LaraverseServiceProvider"
      ],
      "aliases": {
        "Laraverse": "Laraverse\\Facades\\Laraverse"
      }
    }
  }
}
